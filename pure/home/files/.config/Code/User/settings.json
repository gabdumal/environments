{
  "C_Cpp.default.cStandard": "c23",
  "C_Cpp.default.cppStandard": "c++23",
  "C_Cpp.default.intelliSenseMode": "clang-x64",
  "[asciidoc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[handlebars]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "prettier.tabWidth": 2
  },
  "[mdx]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "application.shellEnvironmentResolutionTimeout": 20,
  "cSpell.enableFiletypes": ["mdx"],
  "cSpell.language": "en,en-GB,pt,pt-BR",
  "cSpell.userWords": [
    "Malosto",
    "askama",
    "catppuccin",
    "clsx",
    "dotnet",
    "firefox",
    "LLDB",
    "nixpkgs",
    "plantuml",
    "tailwindcss"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.markdownlint": "explicit"
  },
  "editor.detectIndentation": false,
  "editor.fontFamily": "'MesloLGL Nerd Font Mono', 'FiraCode Nerd Font Mono'",
  "editor.fontSize": 18,
  "editor.fontWeight": "400",
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.mouseWheelZoom": true,
  "editor.selectionClipboard": false,
  "editor.semanticHighlighting.enabled": true,
  "editor.tabSize": 4,
  "editor.wordWrap": "bounded",
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.css": "css",
    "*.mdx": "mdx"
  },
  "files.autoSave": "afterDelay",
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "firefox.keepProfileChanges": true,
  "firefox.profile": "default",
  "git.autofetch": true,
  "git.enableCommitSigning": true,
  "git.openRepositoryInParentFolders": "always",
  "github.copilot.enable": {
    "*": true,
    "markdown": true,
    "plaintext": true
  },
  "github.gitProtocol": "ssh",
  "markdownlint.config": {
    "MD024": false,
    "default": true
  },
  "markdownlint.run": "onSave",
  "plantuml.exportFormat": "svg",
  "plantuml.exportIncludeFolderHeirarchy": false,
  "plantuml.exportOutDir": "docs",
  "plantuml.exportSubFolder": false,
  "plantuml.render": "Local",
  "tailwindCSS.experimental.classRegex": [
    ["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"],
    ["cx\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
  ],
  "terminal.integrated.fontFamily": "'MesloLGS Nerd Font Mono', 'FiraCode Nerd Font Mono'",
  "terminal.integrated.minimumContrastRatio": 1,
  "update.mode": "none",
  "vite.port": 5173,
  "window.newWindowProfile": null,
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 1,
  "workbench.colorTheme": "Catppuccin Latte",
  "workbench.iconTheme": "catppuccin-latte",
  "workbench.settings.applyToAllProfiles": [
    "[asciidoc]",
    "[csharp]",
    "[css]",
    "[handlebars]",
    "[html]",
    "[javascript]",
    "[json]",
    "[jsonc]",
    "[markdown]",
    "[typescript]",
    "[typescriptreact]",
    "application.shellEnvironmentResolutionTimeout",
    "C_Cpp.default.cppStandard",
    "C_Cpp.default.cStandard",
    "C_Cpp.default.intelliSenseMode",
    "C_Cpp.intelliSenseEngine",
    "clang_tidy.additionalOptions",
    "clang_tidy.checks",
    "clang_tidy.enabled",
    "cmake.configureOnOpen",
    "cmake.pinnedCommands",
    "cmake.showOptionsMovedNotification",
    "cSpell.language",
    "cSpell.userWords",
    "debug.javascript.autoAttachFilter",
    "editor.codeActionsOnSave",
    "editor.detectIndentation",
    "editor.fontFamily",
    "editor.fontSize",
    "editor.formatOnSave",
    "editor.inlineSuggest.enabled",
    "editor.minimap.enabled",
    "editor.mouseWheelScrollSensitivity",
    "editor.mouseWheelZoom",
    "editor.selectionClipboard",
    "editor.semanticHighlighting.enabled",
    "editor.tabSize",
    "editor.wordWrap",
    "explorer.confirmDragAndDrop",
    "files.associations",
    "files.autoSave",
    "firefox.executable",
    "firefox.keepProfileChanges",
    "firefox.profile",
    "git.autofetch",
    "git.enableCommitSigning",
    "git.openRepositoryInParentFolders",
    "github.copilot.enable",
    "github.gitProtocol",
    "markdownlint.config",
    "markdownlint.run",
    "plantuml.exportFormat",
    "plantuml.exportIncludeFolderHeirarchy",
    "plantuml.exportOutDir",
    "plantuml.exportSubFolder",
    "plantuml.render",
    "tailwindCSS.experimental.classRegex",
    "terminal.integrated.fontFamily",
    "terminal.integrated.minimumContrastRatio",
    "terminal.integrated.mouseWheelScrollSensitivity",
    "window.newWindowProfile",
    "window.titleBarStyle",
    "window.zoomLevel",
    "workbench.colorTheme",
    "workbench.experimental.enableNewProfilesUI",
    "workbench.iconTheme",
    "workbench.list.mouseWheelScrollSensitivity",
    "workbench.startupEditor"
  ],
  "workbench.startupEditor": "none"
}
